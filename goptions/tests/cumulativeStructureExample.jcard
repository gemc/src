{
	# gsetup is a cumulative structured option: entries can be added on the jcard(s) or at the command line
	#
	# Corresponding command lines:
	#
	# +detector={detector: experiments/clas12/targets; factory: TEXT; variation: bonus;}
	# +detector={detector: experiments/clas12/rtpc; factory: TEXT; variation: rgb_spring2019;}
	# +detector={detector: experiments/clas12/ltcc; factory: CAD;}
	#
	# Notice how some options have default values and could be omitted

	"+gsetup": [
		
		# central detector
		# as an example we define it in-line
		{ "detector": "experiments/clas12/targets", "factory": "TEXT", "variation": "bonus" },
		
		# forward detector
		# as an example we use carriage return to organize the fields
		{
			"detector": "experiments/clas12/rtpc",
			"factory": "TEXT",
			"variation": "rgb_spring2019"
		},
		
		# forward detector
		{
			"detector": "experiments/clas12/ltcc",
			"factory": "CAD"
		}
		
	]
}
